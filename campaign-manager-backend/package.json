{
    "name": "win-ab-testing",
    "version": "1.0.0",
    "description": "A/B Testing for news site",
    "main": "dist/index.js",
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "build": "tsc",
        "dev": "nodemon --exec ts-node src/index.ts",
        "start:dev": "NODE_ENV=development ts-node-dev -r dotenv/config -- path/to/config/dev.env your_server.ts",
        "start:test": "NODE_ENV=testing ts-node-dev -r dotenv/config -- path/to/config/test.env your_server.ts",
        "start:prod": "NODE_ENV=production ts-node-dev -r dotenv/config -- path/to/config/prod.env your_server.ts"
    },
    "keywords": [],
    "author": "Zanele Sibanyoni",
    "dependencies": {
        "axios": "^1.4.0",
        "cheerio": "^1.0.0-rc.12",
        "config": "^3.3.9",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "pg": "^8.6.0",
        "rollbar": "^2.26.1",
        "sanitize-html": "^2.11.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/config": "^3.3.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/pg": "^8.6.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.4"
    }
}
